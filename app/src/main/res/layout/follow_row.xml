<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView13"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/ic_launcher"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/fbrand"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView13"
        android:layout_toEndOf="@+id/imageView13"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/flike"
        android:layout_below="@+id/fbrand"
        android:layout_alignLeft="@+id/fbrand"
        android:layout_alignStart="@+id/fbrand" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/fcate"
        android:layout_alignBottom="@+id/imageView13"
        android:layout_alignLeft="@+id/flike"
        android:layout_alignStart="@+id/flike" />


</RelativeLayout>
